<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>bean</managed-bean-name>
		<managed-bean-class>com.sree.health.Bean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>chartView</managed-bean-name>
		<managed-bean-class>com.sree.health.ChartView</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>areaChartView</managed-bean-name>
		<managed-bean-class>com.sree.health.AreaChartView</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>scheduleView</managed-bean-name>
		<managed-bean-class>com.sree.health.ScheduleView</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
			<redirect>
				<redirect-param>
					<name>faces-redirect</name>
					<value>true</value>
				</redirect-param>
			</redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>customer</from-outcome>
			<to-view-id>/customer.xhtml</to-view-id>
			<redirect>
				<redirect-param>
					<name>faces-redirect</name>
					<value>true</value>
				</redirect-param>
			</redirect>
		</navigation-case>
	</navigation-rule>
</faces-config>
